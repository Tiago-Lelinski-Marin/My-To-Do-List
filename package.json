{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "template with webpack set up",
  "private": true,
  "scripts": {
    "test": "jest",
    "watch": "webpack --watch",
    "see": "webpack serve --open",
    "build": "webpack --config webpack.config.js",
    "bundle-report": "webpack-bundle-analyzer --port 4200 dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tiago-Lelinski-Marin/webpack-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tiago-Lelinski-Marin/webpack-boilerplate/issues"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "homepage": "https://tiago-lelinski-marin.github.io/My-To-Do-List/",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "babel-eslint": "^10.1.0",
    "css-loader": "^6.7.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.26.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "node-sass": "^7.0.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-csstree-validator": "^1.9.0",
    "stylelint-scss": "^3.21.0",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}